{
    "name": "Whatsapp-Bot",
    "description": "Whatsapp-Bot a ReBase from x-Asena",
    "logo": "https://avatars.githubusercontent.com/u/88338865?v=4",
    "keywords": ["Whatsapp", "Userbot", "X-Asena", "X-AsenaDuplicated", "bot", "api", "ai"],
    "repository": "https://github.com/Viper-x0/Whatsapp-Bot",
    "website": "under maintenance",
    "success_url": "under maintenance",
    "stack": "container",
    "env": {
        "HANDLERS": {
            "description": "Bot Prefix Default - ^[.]",
            "required": true,
            "value": "^"
        },
        "SUDO": {
            "description": "Leave blank if you don't know how to use it! Sudo mode; Your number must be an international number. So you should have your number after the country code. Example: 1205xxxxxxxxx (USA number +1)",
            "required": false
        },
        "WORK_TYPE": {
            "description": "Whatsapp-Bot Work Type public/private",
            "required": true,
            "value": "public"
        },
    },
    "buildpacks": [
      {
        "url": "heroku-community/apt",
      },
      {
        "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest",
      },
      {
        "url": "https://github.com/DuckyTeam/heroku-buildpack-imagemagick"
      }
    ]
}
